  Name = "cid",
  Callback = function()
    local args = {[1] = 8, [2] = 1}
    while true do
      game:GetService("Players").LocalPlayer.PlayerGui.Tengen.Skill1b.Z.TextButtonm1.Hitbox:FireServer(unpack(args))
      wait(0)
    end
    print("button pressed")
  end
})

